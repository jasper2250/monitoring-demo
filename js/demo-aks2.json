{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "managedClusters_demo_aks2_name": {
            "defaultValue": "demo-aks2",
            "type": "String"
        },
        "virtualNetworks_demo_vnet_externalid": {
            "defaultValue": "/subscriptions/6c7300fe-0701-437d-b954-6dcbcb876f5f/resourceGroups/demo/providers/Microsoft.Network/virtualNetworks/demo-vnet",
            "type": "String"
        },
        "publicIPAddresses_f5326189_d4b1_4d5c_bc9f_0e5e89edec45_externalid": {
            "defaultValue": "/subscriptions/6c7300fe-0701-437d-b954-6dcbcb876f5f/resourceGroups/MC_demo_demo-aks2_centralus/providers/Microsoft.Network/publicIPAddresses/f5326189-d4b1-4d5c-bc9f-0e5e89edec45",
            "type": "String"
        },
        "userAssignedIdentities_demo_aks2_agentpool_externalid": {
            "defaultValue": "/subscriptions/6c7300fe-0701-437d-b954-6dcbcb876f5f/resourceGroups/MC_demo_demo-aks2_centralus/providers/Microsoft.ManagedIdentity/userAssignedIdentities/demo-aks2-agentpool",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.ContainerService/managedClusters",
            "apiVersion": "2021-07-01",
            "name": "[parameters('managedClusters_demo_aks2_name')]",
            "location": "centralus",
            "sku": {
                "name": "Basic",
                "tier": "Free"
            },
            "identity": {
                "principalId": "4887255d-de90-41b5-af5d-8d699b33a875",
                "tenantId": "a19f121d-81e1-4858-a9d8-736e267fd4c7",
                "type": "SystemAssigned"
            },
            "properties": {
                "kubernetesVersion": "1.20.9",
                "dnsPrefix": "[concat(parameters('managedClusters_demo_aks2_name'), '-dns')]",
                "agentPoolProfiles": [
                    {
                        "name": "agentpool",
                        "count": 1,
                        "vmSize": "Standard_DS2_v2",
                        "osDiskSizeGB": 128,
                        "osDiskType": "Managed",
                        "kubeletDiskType": "OS",
                        "vnetSubnetID": "[concat(parameters('virtualNetworks_demo_vnet_externalid'), '/subnets/default')]",
                        "maxPods": 110,
                        "type": "VirtualMachineScaleSets",
                        "availabilityZones": [
                            "1",
                            "2",
                            "3"
                        ],
                        "enableAutoScaling": false,
                        "orchestratorVersion": "1.20.9",
                        "mode": "System",
                        "osType": "Linux",
                        "osSKU": "Ubuntu",
                        "enableFIPS": false
                    }
                ],
                "windowsProfile": {
                    "adminUsername": "azureuser",
                    "enableCSIProxy": true
                },
                "servicePrincipalProfile": {
                    "clientId": "msi"
                },
                "addonProfiles": {
                    "azurepolicy": {
                        "enabled": false
                    },
                    "httpApplicationRouting": {
                        "enabled": false
                    }
                },
                "nodeResourceGroup": "[concat('MC_demo_', parameters('managedClusters_demo_aks2_name'), '_centralus')]",
                "enableRBAC": true,
                "networkProfile": {
                    "networkPlugin": "azure",
                    "loadBalancerSku": "Standard",
                    "loadBalancerProfile": {
                        "managedOutboundIPs": {
                            "count": 1
                        },
                        "effectiveOutboundIPs": [
                            {
                                "id": "[parameters('publicIPAddresses_f5326189_d4b1_4d5c_bc9f_0e5e89edec45_externalid')]"
                            }
                        ]
                    },
                    "serviceCidr": "10.0.0.0/16",
                    "dnsServiceIP": "10.0.0.10",
                    "dockerBridgeCidr": "172.18.0.1/16",
                    "outboundType": "loadBalancer"
                },
                "apiServerAccessProfile": {
                    "enablePrivateCluster": false
                },
                "identityProfile": {
                    "kubeletidentity": {
                        "resourceId": "[parameters('userAssignedIdentities_demo_aks2_agentpool_externalid')]",
                        "clientId": "08f5aa12-c3c1-420c-a474-759bc8f9cb37",
                        "objectId": "f4589295-317e-4e04-a601-1642f26454a7"
                    }
                }
            }
        },
        {
            "type": "Microsoft.ContainerService/managedClusters/agentPools",
            "apiVersion": "2021-07-01",
            "name": "[concat(parameters('managedClusters_demo_aks2_name'), '/agentpool')]",
            "dependsOn": [
                "[resourceId('Microsoft.ContainerService/managedClusters', parameters('managedClusters_demo_aks2_name'))]"
            ],
            "properties": {
                "count": 1,
                "vmSize": "Standard_DS2_v2",
                "osDiskSizeGB": 128,
                "osDiskType": "Managed",
                "kubeletDiskType": "OS",
                "vnetSubnetID": "[concat(parameters('virtualNetworks_demo_vnet_externalid'), '/subnets/default')]",
                "maxPods": 110,
                "type": "VirtualMachineScaleSets",
                "availabilityZones": [
                    "1",
                    "2",
                    "3"
                ],
                "enableAutoScaling": false,
                "orchestratorVersion": "1.20.9",
                "mode": "System",
                "osType": "Linux",
                "osSKU": "Ubuntu",
                "enableFIPS": false
            }
        }
    ]
}
